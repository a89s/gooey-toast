{
    "name": "gooey/toast",
    "description": "A gooey expandable toast notification component for Laravel, Livewire",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Arturs Sprogis",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0|^10.0",
        "pestphp/pest": "^2.0|^3.0"
    },
    "autoload": {
        "psr-4": {
            "Gooey\\Toast\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gooey\\Toast\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Gooey\\Toast\\GooeyToastServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
